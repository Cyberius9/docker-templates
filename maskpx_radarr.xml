    <Network>bridge</Network>
    <MyIP/>
    <Shell>sh</Shell>
    <Privileged>false</Privileged>
    <Support>http://lime-technology.com/forum/index.php?topic=55549.0</Support>
    <Project>https://github.com/Radarr/Radarr</Project>
    <Overview>&#xD;
    This fork of Sonarr aims to turn it into something like Couchpotato. NOTE This application is under heavy development right now, it may not work correctly.&#xD;
    </Overview>
    <Category>Downloaders: MediaApp:Video</Category>
    <WebUI>http://[IP]:[PORT:7878]/</WebUI>
    <TemplateURL>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/radarr.xml</TemplateURL>
    <Icon>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/images/radarr-icon.png</Icon>
    <ExtraParams/>
    <PostArgs/>
    <CPUset/>
    <DateInstalled>1613592265</DateInstalled>
    <DonateText>If you appreciate my work, then please consider buying me a beer :D</DonateText>
    <DonateLink>https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&amp;amp;hosted_button_id=MM5E27UX6AUU4</DonateLink>
    <Description>&#xD;
    This fork of Sonarr aims to turn it into something like Couchpotato. NOTE This application is under heavy development right now, it may not work correctly.&#xD;
    </Description>
    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
                <HostPort>7878</HostPort>
                <ContainerPort>7878</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>
    <Config Name="Host Port 1" Target="7878" Default="7878" Mode="tcp" Description="Container Port: 7878" Type="Port" Display="always" Required="true" Mask="false">7878</Config>
    <Config Name="Host Path 2" Target="/data" Default="/mnt/user/appdata/data" Mode="rw" Description="Container Path: /data" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/data</Config>
    <Config Name="Host Path 3" Target="/media" Default="/mnt/user" Mode="rw" Description="Container Path: /media" Type="Path" Display="always" Required="true" Mask="false">/mnt/user</Config>
    <Config Name="Key 1" Target="UMASK" Default="000" Mode="" Description="Container Variable: UMASK" Type="Variable" Display="always" Required="false" Mask="false">000</Config>
    <Config Name="Key 2" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
    <Config Name="Key 3" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
    <Config Name="traefik.enable" Target="traefik.enable" Default="true" Mode="" Description="Container Label: traefik.enable" Type="Label" Display="always" Required="true" Mask="false">true</Config>
    <Config Name="traefik.http.routers.radarr.rule" Target="traefik.http.routers.radarr.rule" Default="Host(`radarr.example.com`)" Mode="" Description="Container Label: traefik.http.routers.radarr.rule" Type="Label" Display="always" Required="true" Mask="false"></Config>
    <Config Name="traefik.http.services.radarr.loadbalancer.server.port" Target="traefik.http.services.radarr.loadbalancer.server.port" Default="7878" Mode="" Description="Container Label: traefik.http.services.radarr.loadbalancer.server.port" Type="Label" Display="always" Required="false" Mask="false">7878</Config>
    <Config Name="traefik.http.routers.radarr.middlewares" Target="traefik.http.routers.radarr.middlewares" Default="secured@file" Mode="" Description="Container Label: traefik.http.routers.radarr.middlewares" Type="Label" Display="always" Required="true" Mask="false">secured@file</Config>
    <Config Name="traefik.http.routers.radarr.entrypoints" Target="traefik.http.routers.radarr.entrypoints" Default="https" Mode="" Description="Container Label: traefik.http.routers.radarr.entrypoints" Type="Label" Display="always" Required="true" Mask="false">https</Config>
    <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/binhex-radarr" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/binhex-radarr</Config>

<?xml version="1.0"?>
<Container version="2">
    <Date>2021-02-07</Date>
    <Beta>False</Beta>
    <Repository>traefik</Repository>
    <Registry>https://registry.hub.docker.com/_/traefik<Registry/>
    <Name>Traefik2</Name>
    <Branch>
        <Tag>latest</Tag>
        <TagDescription>Latest stable release of traefik2</TagDescription>
    </Branch>
    <Branch>
        <Tag>v2.4</Tag>
        <TagDescription>Fixed to Version 2.4 Releases</TagDescription>
    </Branch>
    <Branch>
        <Tag>v1.7</Tag>
        <TagDescription>Deprecated old 1.7 Releases</TagDescription>
    </Branch>
    <Network Default="host">host</Network>
    <MyIP/>
    <Shell>sh</Shell>
    <Privileged>false</Privileged>
    <BindTime>true</BindTime>
    <Support>https://github.com/maskpx/docker-templates/issues</Support>
    <Project>https://traefik.io/traefik/</Project>
    <Overview>Traefik is a modern reverse-proxy.</Overview>
    <Category>Network:Web, Network:Proxy</Category>
    <WebUI>http://[IP]:[PORT:8080]</WebUI>
    <TemplateURL>https://raw.githubusercontent.com/maskpx/docker-templates/master/maskpx_traefik.xml</TemplateURL>
    <Icon>https://raw.githubusercontent.com/maskpx/docker-templates/master/images/traefik.png</Icon>
    <ExtraParams/>
    <PostArgs/>
    <CPUset/>
    <DonateText/>
    <DonateLink/>
    <Description>Traefik is a modern reverse-proxy.</Description>
    <Networking>
        <Mode>host</Mode>
        <Publish>
            <Port>
                <HostPort>80</HostPort>
                <ContainerPort>80</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
            <Port>
                <HostPort>443</HostPort>
                <ContainerPort>443</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
            <Port>
                <HostPort>8080</HostPort>
                <ContainerPort>8080</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>
    <Data>
        <Volume>
            <HostDir>/mnt/user/appdata/traefik</HostDir>
            <ContainerDir>/etc/traefik</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
        <Volume>
            <HostDir>/var/run/docker.sock</HostDir>
            <ContainerDir>/var/run/docker.sock</ContainerDir>
            <Mode>ro</Mode>
        </Volume>
    </Data>
    <Environment>
        <Variable>
            <Name>CF_API_EMAIL</Name>
            <Value></Value>
        </Variable>
        <Variable>
            <Name>CF_API_KEY</Name>
            <Value></Value>
        </Variable>
    </Environment>
    <Labels>
        <Label>
            <Value>true</Value>
            <Name>traefik.enable</Name>
            <Mode/>
        </Label>
        <Label>
            <Value>http|https</Value>
            <Name>traefik.http.routers.api.entrypoints</Name>
            <Mode/>
        </Label>
        <Label>
            <Value></Value>
            <Name>traefik.http.routers.api.rule</Name>
            <Mode/>
        </Label>
        <Label>
            <Value>api@internal</Value>
            <Name>traefik.http.routers.api.service</Name>
            <Mode/>
        </Label>
        <Label>
            <Value>secured@file|secHeaders@file|emby@file</Value>
            <Name>traefik.http.routers.traefik.middlewares</Name>
            <Mode/>
        </Label>
    </Labels>
</Container> 